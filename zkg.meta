[package]
summary = Spicy-based analyzer for the PostgreSQL protocol
description = Code was taken directly from Zeek's tree.
script_dir = analyzer
plugin_dir = build/spicy-modules
build_command = mkdir -p build && cd build && SPICYZ=$(command -v spicyz || (echo "spicyz not found" >&2 && exit 1)) cmake .. && cmake --build .
test_command = cd tests && PATH=$(zkg config plugin_dir)/packages/spicy-plugin/bin:$PATH btest -d -j $(nproc)
